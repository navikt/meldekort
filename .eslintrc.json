{
  "parser": "@typescript-eslint/parser",
  "plugins": ["react", "react-hooks"],
  "rules": {
    "no-useless-computed-key": "off",
    "array-callback-return": "off",
    "no-useless-concat": "off"
  },
  "overrides": [
    {
      "files": ["*.tsx"],
      "rules": {
        "no-useless-computed-key": "off",
        "array-callback-return": "off",
        "no-useless-concat": "off"
      }
    }
  ]
}