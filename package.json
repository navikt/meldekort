  "scripts": {
    "build": "set CI=false && craco build",
    "eject": "craco eject",
    "start": "cross-env REACT_APP_MOCK=true craco start",
    "start:mock": "cross-env REACT_APP_MOCK_FULL=true npm run start",
    "test": "react-scripts test --env=jsdom --coverage --watchAll=false --w=1",
    "test:watch": "react-scripts test --env=jsdom",
    "test:reload": "react-scripts test -u"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "ie >= 10",
    "not op_mini all"
  ],
  "devDependencies": {
    "@craco/craco": "^7.1.0",
    "@types/amplitude-js": "^8.16.5",
    "@types/enzyme": "^3.10.18",
    "@types/jest": "^29.5.11",
    "@types/ramda": "^0.29.9",
    "@types/react": "^17.0.71",
    "@types/react-aria-menubutton": "^6.2.14",
    "@types/react-collapse": "^5.0.4",
    "@types/react-dom": "^17.0.25",
    "@types/react-intl-redux": "^0.1.19",
    "@types/react-redux": "^7.1.33",
    "@types/react-test-renderer": "^17.0.9",
    "@types/redux-mock-store": "^1.0.6",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.8.0",
    "axios-mock-adapter": "^1.22.0",
    "cross-env": "^7.0.3",
    "enzyme": "^3.11.0",
    "enzyme-to-json": "^3.6.2",
    "eslint": "^8.56.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "react-test-renderer": "^17.0.2",
    "redux-mock-store": "^1.5.4",
    "typescript": "^5.3.3"
  },
  "optionalDependencies": {
    "fsevents": "^2.3.3"
  },
  "eslintConfig": {
    "rules": {
      "no-useless-computed-key": "off",
      "array-callback-return": "off",
      "no-useless-concat": "off"
    }
  },
  "snyk": true,
  "jest": {
    "moduleNameMapper": {
      "axios": "axios/dist/node/axios.cjs"
    },
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "transformIgnorePatterns": [
      "/node_modules/(?!intl-messageformat|intl-messageformat-parser).+\\.js$"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix"
    ],
    "*.tsx": [
      "eslint --fix"
    ]
  }
}
